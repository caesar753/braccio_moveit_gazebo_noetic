<?xml version="1.0"?>
<launch>

<arg name="robot_name" default="kuka" />
<arg name="use_ros_control" default="True" />
<arg name="motors_controller_type" default="EffortJointInterface" />

<!-- Load robot description to parameter server -->

<!-- <param name="camera_description" command="$(find xacro)/xacro '$(find realsense2_description)/urdf/test_d435_camera.urdf.xacro' use_nominal_extrinsics:=true add_plug:=true use_mesh:=true" /> -->

<param name="camera_description" command="$(find xacro)/xacro '$(find braccio_description)/urdf/xacros/_d435.urdf.xacro' " />

<param name="robot_description" textfile="$(find braccio_description)/urdf/braccio_model.urdf" />

<param name="robot_description_gz"  command="$(find xacro)/xacro $(find braccio_description)/urdf/xacros/camera_and_mount_and_braccio.urdf.xacro"/>

<param name="camera_mount_description" textfile="$(find braccio_description)/urdf/camera_mount_model.urdf" /> 


</launch>
